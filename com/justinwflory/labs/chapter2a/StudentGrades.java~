//**********************************************************
//  StudentGrades.java
//
//  Print the grades of students in a class.
//**********************************************************

public class StudentGrades
{
    public static void main(String[] args)
    {
      System.out.println ();
      System.out.println ("///////////////////\\\\\\\\\\\\\\\\\\\"\\\\\\\\\\\\\\\\\\\");
      System.out.println ("==                    Student Points                    ==");
      System.out.println ("///////////////////\\\\\\\\\\\\\\\\\\\"\\\\\\\\\\\\\\\\\\\");
      System.out.println ("\tAlexander\t\tWashington\t\tMath");
      System.out.println ("\tJustin\t\tTucker\t\tComputer Science, Networking");
      System.out.println ("\tSamanyu\t\tHyderabad\t\tUnknown");
      System.out.println ("\tZach\t\tCumming\t\tUnknown");
      System.out.println ();
    }
}